# Прокси для обхода блокировок (опционально)
PROXY=

# Базовая задержка между запросами в секундах
REQUEST_DELAY_BASE=0.8

# Случайный джиттер для задержек в секундах
REQUEST_DELAY_JITTER=0.4

# User-Agent для запросов (опционально, будет использоваться случайный)
USER_AGENT=

# Максимальное количество попыток при ошибках
MAX_RETRIES=3